cc_version: gcc (GCC) 11.2.0
cxx_version: g++ (GCC) 11.2.0
fc_version: GNU Fortran (GCC) 11.2.0
nvcc_version: 

cflags: -funsigned-char -W -Wall -Wshadow -Wpointer-arith -Wcast-qual -Wcast-align -Wwrite-strings -Wstrict-prototypes -Wmissing-prototypes -Wmissing-declarations -Wnested-externs -Wunused -Wfloat-equal -Werror=implicit-function-declaration -Wmisleading-indentation -Wduplicated-cond -fdiagnostics-color=auto -Werror=format-security  -fopenmp  -O2 -fexcess-precision=fast
