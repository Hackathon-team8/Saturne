fcflags: -x f95-cpp-input -Wall -pedantic-errors -std=f2008 -fdiagnostics-color=auto  -fopenmp  -O
nvccflags:   

cppflags:   -D_POSIX_C_SOURCE=200809L -DNDEBUG 
ldflags:   -O -Wl,-export-dynamic -fopenmp 
libs: -lz   -ldl -L/opt/tools-hackathon/arm/compilers/gcc-11.2.0_Generic-AArch64_RHEL-7_aarch64-linux/lib/gcc/aarch64-linux-gnu/11.2.0 -L/opt/tools-hackathon/arm/compilers/gcc-11.2.0_Generic-AArch64_RHEL-7_aarch64-linux/lib/gcc -L/lib64 -L/usr/lib64 -L/opt/tools-hackathon/arm/compilers/armpl-22.1.0_AArch64_RHEL-7_gcc_aarch64-linux/lib -L/opt/tools-hackathon/arm/compilers/gcc-11.2.0_Generic-AArch64_RHEL-7_aarch64-linux/lib64 -L/opt/tools-hackathon/arm/compilers/gcc-11.2.0_Generic-AArch64_RHEL-7_aarch64-linux/aarch64-linux-gnu/lib -L/opt/tools-hackathon/arm/compilers/gcc-11.2.0_Generic-AArch64_RHEL-7_aarch64-linux/lib -lgfortran -lm

